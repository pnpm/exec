{
  "name": "@pnpm/exec",
  "version": "1.1.5",
  "description": "Executes pnpm. If pnpm is not installed, installs it first",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "md": "mos",
    "lint": "tslint -c tslint.json --project .",
    "pretest": "preview",
    "test": "mos test && npm run lint && rimraf test/fixture/node_modules && ts-node test --type-check",
    "prepublishOnly": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pnpm/exec.git"
  },
  "keywords": [
    "pnpm",
    "exec"
  ],
  "author": {
    "name": "Zoltan Kochan",
    "email": "z@kochan.io",
    "url": "https://www.kochan.io/",
    "twitter": "ZoltanKochan"
  },
  "engines": {
    "node": ">=4"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pnpm/exec/issues"
  },
  "homepage": "https://github.com/pnpm/exec#readme",
  "devDependencies": {
    "@types/tape": "^4.2.31",
    "mos": "^2.0.0-alpha.3",
    "mos-plugin-readme": "^1.0.4",
    "package-preview": "^1.0.0",
    "rimraf": "^2.6.2",
    "tape": "^4.8.0",
    "ts-node": "^8.0.1",
    "tslint": "^5.8.0",
    "typescript": "^3.0.0"
  },
  "mos": {
    "plugins": [
      "readme"
    ],
    "installation": {
      "useShortAlias": true
    }
  },
  "dependencies": {
    "@pnpm/self-installer": "^2.0.0",
    "@types/got": "^8.3.1",
    "@types/node": "^10.0.8",
    "command-exists": "^1.2.2",
    "cross-spawn": "^7.0.0"
  }
}
